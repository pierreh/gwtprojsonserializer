<?xml version="1.0"?>
<project name="GWTProJsonSerializer"
	basedir=".."
	xmlns:module.common="http://kfuntak.com/gwt/ant/module/common"
	xmlns:module.compile="http://kfuntak.com/gwt/ant/module/compile"
	xmlns:module.deploy="http://kfuntak.com/gwt/ant/module/deploy"
	xmlns:contrib="http://antcontrib.sf.net"
	default="usage">
	
	<taskdef resource="net/sf/antcontrib/antcontrib.properties"/>
	
	<import file="modules/common.ant"/>
	<import file="modules/compile.ant"/>
	
	<description>
        This is main build file for GWTProJsonSerializer.
    </description>

	<target name="usage">
		<echo>
			This is main build file for GWTProJsonSerializer.
        </echo>
	</target>

	
	<target name="clean" >
		<module.common:init />
		<runtarget target="common.clean" />
	</target>
	
	<target name="compile" >
		<module.compile:init />
		<runtarget target="compile.compile" />
	</target>
	
	<target name="jar" >
		<module.compile:init />
		<runtarget target="compile.jar" />
	</target>
	
	<target name="build" >
		<runtarget target="clean" />
		<runtarget target="compile" />
		<runtarget target="jar" />
	</target>
	
</project>
